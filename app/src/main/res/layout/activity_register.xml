<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/editTextText8"
        android:layout_width="165dp"
        android:layout_height="46dp"
        android:layout_marginBottom="44dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/calendarView2"
        app:layout_constraintStart_toStartOf="@+id/calendarView2"
        app:layout_constraintTop_toTopOf="@+id/editTextText7" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="165dp"
        android:layout_height="19dp"
        android:text="Nome"
        app:layout_constraintBottom_toTopOf="@+id/editTextText8"
        app:layout_constraintStart_toStartOf="@+id/editTextText8" />

    <EditText
        android:id="@+id/editTextText7"
        android:layout_width="166dp"
        android:layout_height="46dp"
        android:layout_marginBottom="44dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/calendarView2"
        app:layout_constraintEnd_toEndOf="@+id/calendarView2" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="166dp"
        android:layout_height="19dp"
        android:text="Cognome"
        app:layout_constraintBottom_toTopOf="@+id/editTextText7"
        app:layout_constraintStart_toStartOf="@+id/editTextText7" />

    <CalendarView
        android:id="@+id/calendarView2"
        android:layout_width="378dp"
        android:layout_height="322dp"
        android:layout_marginEnd="18dp"
        android:layout_marginBottom="148dp"
        app:layout_constraintBottom_toTopOf="@+id/button5"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="210dp"
        android:layout_height="wrap_content"
        android:text="Data di Nascita"
        app:layout_constraintBottom_toTopOf="@+id/calendarView2"
        app:layout_constraintStart_toStartOf="@+id/calendarView2" />

    <EditText
        android:id="@+id/editTextTextPassword4"
        android:layout_width="174dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/calendarView2"
        app:layout_constraintTop_toBottomOf="@+id/textView15" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="174dp"
        android:layout_height="21dp"
        android:text="User"
        app:layout_constraintBottom_toTopOf="@+id/editTextText10"
        app:layout_constraintStart_toStartOf="@+id/editTextText10" />

    <EditText
        android:id="@+id/editTextText10"
        android:layout_width="174dp"
        android:layout_height="44dp"
        android:layout_marginTop="68dp"
        android:ems="10"
        android:inputType="text"
        app:layout_constraintStart_toStartOf="@+id/calendarView2"
        app:layout_constraintTop_toBottomOf="@+id/calendarView2" />

    <TextView
        android:id="@+id/textView16"
        android:layout_width="173dp"
        android:layout_height="21dp"
        android:text="Password"
        app:layout_constraintBottom_toTopOf="@+id/editTextTextPassword4"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword4"
        app:layout_constraintStart_toStartOf="@+id/editTextTextPassword4" />

    <Button
        android:id="@+id/button5"
        android:layout_width="267dp"
        android:layout_height="43dp"
        android:layout_marginBottom="64dp"
        android:baselineAligned="false"
        android:text="Register"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/editTextTextPassword4"
        app:layout_constraintStart_toStartOf="@+id/editTextText10" />

</androidx.constraintlayout.widget.ConstraintLayout>